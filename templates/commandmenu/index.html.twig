{% extends 'base.html.twig' %}

{% block title %}Commandmenu index{% endblock %}

{% block body %}
    <h1>Commandmenu index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Quantity</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for commandmenu in commandmenus %}
            <tr>
                <td>{{ commandmenu.id }}</td>
                <td>{{ commandmenu.quantity }}</td>
                <td>
                    <a href="{{ path('commandmenu_show', {'id': commandmenu.id}) }}">show</a>
                    <a href="{{ path('commandmenu_edit', {'id': commandmenu.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('commandmenu_new') }}">Create new</a>
{% endblock %}
